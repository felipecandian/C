#include<stdio.h>
#include<stdlib.h>


int matrizTriangular (int matriz[4][4])
{
    int i, j;

    for ( i = 0 ; i < 4 ; i++ )
    {
        for ( j = 0 ; j < 4 ; j++ )
        {
            if i > j
            {
                if ( matriz[i][j] != 0 )
                return 0 ;
            }
        }
    } 
    return 1;
}

int main()
{
    int i , j, matriz[4][4];
    printf("Teste");  

}
